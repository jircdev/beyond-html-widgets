{
    "name": "html-widgets",
    "version": "0.0.1",
    "title": "HTML Widgets for BeyondJS",
    "description": "",
    "license": "ISC",
    "layout": "main-layout",
    "template": "template/template.json",
    "languages": {
        "default": "en",
        "supported": [
            "en",
            "es"
        ]
    },
    "modules": {
        "path": "modules"
    },
    "deployment": {
        "distributions": [
            {
                "name": "web",
                "environment": "development",
                "platform": "web",
                "ssr": "web-ssr",
                "ports": {
                    "bundles": 4530
                }
            },
            {
                "name": "web-ssr",
                "environment": "development",
                "platform": "ssr",
                "ports": {
                    "bundles": 4630
                }
            }
        ]
    },
    "dependencies": {
        "@beyond-js/kernel": "~0.1.8",
        "@beyond-js/local": "~0.1.3",
        "@beyond-js/react-18-widgets": "~0.0.1",
        "@beyond-js/widgets": "0.1.2",
        "socket.io-client": "^4.5.4"
    },
    "devDependencies": {
        "@types/react": "^18.0.26",
        "@types/react-dom": "^18.0.9"
    }
}
